
示例GET请求：
GET http://127.0.0.1:10088/hand/status

//状态查询示例请求
{
    "taskId": "get_hand_status_1XXX",                           //string - 任务ID
    "taskType": "get_hand_status",                              //string - 任务类型
    "taskParameters": 
    {
	    "part": ["left_hand","right_hand"]                      // Array<string> - 指定需要查询的手部
    }
}

//状态查询示例响应
{
    "taskId": "get_hand_status_1XXX",                           // string - 任务ID             
    "taskType": "get_status",                                   // string - 任务类型
    "taskStatus": "FINISHED",                                   // string - 任务状态
    "taskResult": {                                             // object - 任务结果
        "left_hand": {                                          // object - 左手状态
            "status": "ONLINE",                                 // string - ONLINE/OFFLINE 
            "fingers":[                                         // array<object> - 手指数组
                {   "name": "thumb",                            // string - 手指名称            
                    "joints":[0.001, 0.001, 0.001, 0.001],      // array<double> - 关节角度 [0,1]
                    "motorForce":[0.001, 0.001, 0.001, 0.001],  // array<double> - 电机传感器 [0,1]
                    "fingerTip_force": [0.001, 0.001, 0.001, 0.001] // array<double> - 三维力（单位N）
                },
                {   "name": "Index",                      
                    "joints":[0,0,0,0],
                    "motorForce":[0,0,0,0],
                    "fingerTip_force": [0,0,0]
                },
                {   "name":"Middle",                       
                    "joints":[0,0,0,0],
                    "motorForce":[0,0,0,0],
                    "fingerTip_force": [0,0,0]
                },
                {   "name": "Ring",                        
                    "joints":[0,0,0,0],
                    "motorForce":[0,0,0,0],
                    "fingerTip_force": [0,0,0]
                },
                {   "name": "Pinky",                      
                    "joints":[0,0,0,0],
                    "motorForce":[0,0,0,0],
                    "fingerTip_force": [0,0,0]
                }
            ]
        },
        "right_hand": {                             
            "status": "ONLINE", 
            "fingers":[                            
                {   "name": "thumb",                          
                    "joints":[0,0,0,0],
                    "motorForce":[0,0,0,0],
                    "fingerTip_force": [0,0,0]
                },
                {   "name": "Index",                      
                    "joints":[0,0,0,0],
                    "motorForce":[0,0,0,0],
                    "fingerTip_force": [0,0,0]
                },
                {   "name":"Middle",                       
                    "joints":[0,0,0,0],
                    "motorForce":[0,0,0,0],
                    "fingerTip_force": [0,0,0]
                },
                {   "name": "Ring",                        
