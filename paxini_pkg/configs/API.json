                    "joints":[0.001, 0.001, 0.001, 0.001]
                },
                {   "name": "Pinky",                      
                    "joints":[0.001, 0.001, 0.001, 0.001]
                }
            ]
        },
        "right_hand": {                             
            "fingers":[
                {   "name": "thumb",  
                    "joints":[0.001, 0.001, 0.001, 0.001]
