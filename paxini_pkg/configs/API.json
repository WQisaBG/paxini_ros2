                    "fingerTip_force": [0.1, 0.1, 0.1]
                }
            ]
        },
        "right_hand": {                             
            "status": "ONLINE", 
            "fingers":[                            
                {   "name": "thumb",                          
                    "joints":[0.001, 0.001, 0.001, 0.001],
                    "motorForce":[0.001, 0.001, 0.001, 0.001],
                    "fingerTip_force": [0.1, 0.1, 0.1]
                },
                {   "name": "Index",                      
